#!/bin/bash
set -e
cd ~/today/forever/data-f9e6c95/data/
sudo tar czfv ../debian.tgz debian start-debian2.sh --exclude-vcs
adb push ../debian.tgz /data
adb 'cd /data; tar zxvf debian.tgz'
ssh-to-phone echo hello
if test "$1" = debian; then
    exit
fi
sudo tar czfv ../data.tgz . &
sleep 10
remote-cmd root@phone 'mkdir -p ~/data; sshfs bhj@192.168.15.33:/home/bhj/today/forever/data-f9e6c95/ ~/data; cd /android/data; tar zxvf ~/data/data.tgz'
