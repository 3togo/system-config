#!/bin/bash
adb remount
if test "$1" != -n; then
    (cd $(android-env get_abs_build_var TARGET_OUT ); adb-push ./lib/modules/* /system/lib/modules) #android-env adb sync system
fi
adb shell setprop persist.sys.usb.config adb
adb -w shell echo xx
adb disconnect
adb -w shell echo xx
adb reboot bootloader
cd $(lookup-file .repo)/..
fastboot flash boot out/target/product/msm8974sfo/boot.img
fastboot flash aboot ./out/target/product/msm8974sfo/emmc_appsboot.mbn
fastboot reboot
