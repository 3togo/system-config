#!/bin/bash
adb remount
(cd $(android-env get_abs_build_var TARGET_OUT ); adb_push $(find . -name '*.ko') /system/lib/modules) #android-env adb sync system
adb reboot bootloader
cd $(lookup-file .repo)/..
fastboot flash boot out/target/product/msm8974sfo/boot.img
fastboot flash aboot ./out/target/product/msm8974sfo/emmc_appsboot.mbn
fastboot reboot
