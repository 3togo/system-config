<externaltool id="vi">
    <description>Opens the current file in e</description>
    <displayname>Edit with Emacs</displayname>
    <category>Text</category>
    <executable output="showinpane" error="showinpane" modifiesdocument="yes">
        <path>e</path>
        <arguments>&quot;%{CurrentDocument:FilePath}&quot;:%{CurrentDocument:Row}</arguments>
        <workingdirectory>%{CurrentDocument:Path}</workingdirectory>
    </executable>
</externaltool>
